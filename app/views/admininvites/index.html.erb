<h1>Listing AdminInvites</h1>

<table  class="table table-striped">
  <tr>
    <th>ID</th>
    <th>first name</th>
    <th>last name</th>
    <th>Recip email</th>
    <th>event id</th>
    <th> token </th>
  </tr>

<% @ais.each do |k| %>
  <tr>
    <td><%= k.id %></td>
    <td><%= k.first_name %></td>
    <td><%= k.last_name %></td>
    <td><%= k.recipient_email %></td>
    <td><%= k.event_id %></td>
    <td><%= k.token %></td>
<% end %>
</table>

