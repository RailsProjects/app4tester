.PlanPageContent{margin: 30px 30px 0px 30px; overflow:hidden}

.PlanBox { margin: 40px 20px; overflow:hidden;}

.PlanBox1 { border-radius: 5px 5px 0px 0px; -moz-border-radius: 5px 5px 0px 0px; -webkit-border-radius: 5px 5px 0px 0px; background:#333; width:170px; margin-left:5px; }

.PlanBoxGold { border-radius: 5px 5px 0px 0px; -moz-border-radius: 5px 5px 0px 0px; -webkit-border-radius: 5px 5px 0px 0px; background:#093; width:170px; margin-left:5px;}

.PlanBoxHeading {color:#fff; padding:10px; font: Bold 22px 'calibri'; color:#FFF;}
