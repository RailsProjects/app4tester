<h1>Listing AdminKeys</h1>

<table  class="table table-striped">
  <tr>
    <th>ID</th>
    <th>User ID</th>
    <th>Event ID</th>
    
  </tr>

<% @keys.each do |k| %>
  <tr>
        <td><%= k.id %></td>
    <td><%= k.user_id %></td>
    <td><%= k.event_id %></td>
    <td><%= link_to 'Destroy', k, method: :delete, data: { confirm: 'Are you sure?' } %></td>
<% end %>
</table>

<br />
