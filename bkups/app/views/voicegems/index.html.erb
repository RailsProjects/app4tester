<h1>Listing voicegems</h1>

<table>
  <tr>
    <th>User</th>
    <th>Event</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>Admin notes</th>
    <th>Vg userinvite</th>
    <th>Admin</th>
    <th>Token</th>
    <th>Recording</th>
    <th>Request</th>
    <th>Notes</th>
    <th>Hidden</th>
    <th>Length</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @voicegems.each do |voicegem| %>
  <tr>
    <td><%= voicegem.user_id %></td>
    <td><%= voicegem.event_id %></td>
    <td><%= voicegem.first_name %></td>
    <td><%= voicegem.last_name %></td>
    <td><%= voicegem.email %></td>
    <td><%= voicegem.admin_notes %></td>
    <td><%= voicegem.vg_userinvite_id %></td>
    <td><%= voicegem.admin_id %></td>
    <td><%= voicegem.token %></td>
    <td><%= voicegem.recording %></td>
    <td><%= voicegem.request %></td>
    <td><%= voicegem.notes %></td>
    <td><%= voicegem.hidden %></td>
    <td><%= voicegem.length %></td>
    <td><%= link_to 'Show', voicegem %></td>
    <td><%= link_to 'Edit', edit_voicegem_path(voicegem) %></td>
    <td><%= link_to 'Destroy', voicegem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

